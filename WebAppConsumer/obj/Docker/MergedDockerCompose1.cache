networks:
  mynetwork:
    driver: bridge
services:
  postgresserver:
    environment:
      POSTGRES_PASSWORD: zafrul
    image: zafrulpostgres
    networks:
      mynetwork: null
    ports:
    - 5432:5432/tcp
    restart: always
    volumes:
    - pgdata:/var/lib/postgresql/data:rw
  webappconsumer:
    build:
      args:
        source: obj/Docker/empty/
      context: D:\Docker\AdfenixSample\WebAppConsumer\WebAppConsumer
      dockerfile: Dockerfile
    depends_on:
    - postgresserver
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
    image: webappconsumer:dev
    labels:
      com.microsoft.visualstudio.debuggee.killprogram: /bin/bash -c "if PID=$$(pidof
        -x dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    links:
    - postgresserver
    networks:
      mynetwork: null
    ports:
    - 5001:80/tcp
    - 80/tcp
    volumes:
    - D:\Docker\AdfenixSample\WebAppConsumer\WebAppConsumer:/app:rw
    - C:\Users\Shahriar\vsdbg:/remote_debugger:ro
version: '3.0'
volumes:
  pgdata: {}